{
  "name": "faatere",
  "private": true,
  "license": "UNLICENSED",
  "workspaces": [
    "frontend",
    "mobile",
    "services/backend",
    "shared",
    "documentation"
  ],
  "scripts": {
    "postinstall": "husky",
    "dev": "yarn workspace backend dev",
    "dev:frontend": "yarn workspace frontend dev",
    "dev:backend": "yarn workspace backend dev",
    "build": "yarn workspaces run build",
    "lint": "yarn workspaces run lint",
    "test": "yarn workspaces run test",
    "compile": "yarn workspaces run compile",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:build": "docker-compose build",
    "docker:ps": "docker-compose ps",
    "db:up": "docker-compose up -d postgres postgres-test minio adminer",
    "db:down": "docker-compose down postgres postgres-test minio adminer",
    "docs": "yarn workspace documentation start",
    "docs:build": "yarn workspace documentation build"
  },
  "devDependencies": {
    "husky": "^9.1.7"
  },
  "packageManager": "yarn@1.22.22"
}
